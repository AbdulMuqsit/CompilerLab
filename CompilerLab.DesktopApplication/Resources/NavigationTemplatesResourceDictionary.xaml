<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CompilerLab.DesktopApplication.Resources"
                    xmlns:vm="clr-namespace:CompilerLab.DesktopApplication.ViewModels"
                    xmlns:view="clr-namespace:CompilerLab.DesktopApplication.Views">
    <DataTemplate  DataType="{x:Type vm:MainViewModel}">
        <view:MainView></view:MainView>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type vm:PFNViewModel}">
        <view:PFNView></view:PFNView>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type vm:RegexExamplesViewModel}">
        <view:RegexExamplesView></view:RegexExamplesView>
    </DataTemplate>
</ResourceDictionary>