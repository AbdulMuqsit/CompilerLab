<Window x:Class="CompilerLab.PFN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CompilerLab.PFN"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"></RowDefinition>
            <RowDefinition Height="20"></RowDefinition>
            <RowDefinition Height="20"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Name="TxtInput"  Grid.Row="0"></TextBox>
        <Button Name="BtnEvaluate" Click="BtnEvaluate_Click"  Grid.Row="1"></Button>
        <TextBox Name="TxtOutput" Grid.Row="2"></TextBox>
    </Grid>
</Window>
